# http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.java]
indent_size = 4
max_line_length = 120

[*.bat]
end_of_line = crlf

[{Makefile,go.mod,go.sum,*.go}]
indent_style = tab
indent_size = 2
